{
	"name":        "semitwistweb",
	"description": "A usable-but-unpolished Vibe.d/mustache-based web framework.",
	"homepage":    "http://github.com/Abscissa/SemiTwistWeb",
	"authors":     ["Nick Sabalausky"],
	"license":     "zlib/libpng",
	"copyright":   "Â©2013-2015 Nick Sabalausky",

	"targetType": "sourceLibrary",
	"sourcePaths": ["src"],
	"importPaths": ["src"],
	"stringImportPaths": ["res"],
	"targetPath": "bin",
	"copyFiles": ["res/*", "www-static/*"],

	"dependencies": {
		"mustache-d": "==0.1.1",
		"mysql-native": "==0.1.3",
		"semitwistdtools": "==0.10.3",
		"vibe-d": "~>0.7.23"
	},

	"configurations": [
		{
			"name": "sdlang",
			"versions": ["SDLang_TestApp"]
		},

		{
			"name": "unittest",
			"targetName": "semitwistweb-unittest",
			"versions": ["SDLang_Unittest", "SDLang_Trace"],
			"dflags-dmd": ["-debug", "-g", "-unittest"]
		}
	]
}
